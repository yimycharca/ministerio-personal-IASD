<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Mipes</servlet-name>
		<servlet-class>mipes.MipesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Mipes</servlet-name>
		<url-pattern>/mipes</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Save</servlet-name>
		<servlet-class>mipes.Save</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Save</servlet-name>
		<url-pattern>/save</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Update</servlet-name>
		<servlet-class>mipes.Update</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Update</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Remove</servlet-name>
		<servlet-class>mipes.Remove</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Remove</servlet-name>
		<url-pattern>/remove</url-pattern>
	</servlet-mapping>
	
	<servlet>
      <servlet-name>GetList</servlet-name>
      <servlet-class>mipes.GetList</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>GetList</servlet-name>
      <url-pattern>/getList</url-pattern>
   </servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
